@article{20f3f5c9-06ba-3425-8213-dca64b652f18,
 ISSN = {00028282},
 URL = {http://www.jstor.org/stable/2117972},
 abstract = {We provide a theoretical interpretation of two features of international data: the countercyclical movements in net exports and the tendency for the trade balance to be negatively correlated with current and future movements in the terms of trade, but positively correlated with past movements. We document the same properties in a two-country stochastic growth model in which trade fluctuations reflect, in large part, the dynamics of capital formation. We find that our general-equilibrium perspective is essential: the relation between the trade balance and the terms of trade depends critically on the source of fluctuations.},
 author = {David K. Backus and Patrick J. Kehoe and Finn E. Kydland},
 journal = {The American Economic Review},
 number = {1},
 pages = {84--103},
 publisher = {American Economic Association},
 title = {Dynamics of the Trade Balance and the Terms of Trade: The J-Curve?},
 urldate = {2023-10-05},
 volume = {84},
 year = {1994}
}

